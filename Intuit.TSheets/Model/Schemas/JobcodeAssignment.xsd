{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "JobcodeAssignment",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "user_id",
    "jobcode_id",
    "active",
    "last_modified",
    "created"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "format": "int32"
    },
    "user_id": {
      "type": "integer",
      "format": "int32"
    },
    "jobcode_id": {
      "type": "integer",
      "format": "int32"
    },
    "active": {
      "type": "boolean"
    },
    "last_modified": {
      "type": "string",
      "format": "date-time"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    }
  }
}