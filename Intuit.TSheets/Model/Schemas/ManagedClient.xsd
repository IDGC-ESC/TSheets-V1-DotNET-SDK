{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ManagedClient",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "company_url",
    "company_name",
    "active",
    "created",
    "last_modified"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "format": "int64"
    },
    "company_url": {
      "type": "string"
    },
    "company_name": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "last_modified": {
      "type": "string",
      "format": "date-time"
    }
  }
}