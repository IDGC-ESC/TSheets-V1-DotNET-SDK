{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TimeOffRequest",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "user_id",
    "time_off_request_notes",
    "time_off_request_entries",
    "status",
    "active",
    "created",
    "last_modified"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "format": "int64"
    },
    "user_id": {
      "type": "integer",
      "format": "int64"
    },
    "time_off_request_notes": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "int64"
      }
    },
    "time_off_request_entries": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "int64"
      }
    },
    "status": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "last_modified": {
      "type": "string",
      "format": "date-time"
    }
  }
}