{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ScheduleCalendar",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "name",
    "last_modified",
    "created"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "format": "int32"
    },
    "name": {
      "type": "string"
    },
    "last_modified": {
      "type": "string",
      "format": "date-time"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    }
  }
}