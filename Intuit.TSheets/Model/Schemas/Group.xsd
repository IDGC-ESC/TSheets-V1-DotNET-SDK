{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Group",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "active",
    "name",
    "manager_ids",
    "created",
    "last_modified"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "format": "int64"
    },
    "active": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "manager_ids": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "int64"
      }
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "last_modified": {
      "type": "string",
      "format": "date-time"
    }
  }
}